<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500" prefWidth="400" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.SystemController">
   <children>
      <Label graphicTextGap="5.0" layoutX="57.0" layoutY="26.0" prefHeight="76.0" prefWidth="285.0" text="Administrator home page " textFill="#652929">
         <font>
            <Font size="23.0" />
         </font>
         <effect>
            <Glow level="0.39" />
         </effect>
      </Label>
      <Button fx:id="AddMemberButton" blendMode="DARKEN" layoutX="32.0" layoutY="200.0" mnemonicParsing="false" onAction="#addMemberEntry" prefHeight="54.0" prefWidth="161.0" text="Add library member" textFill="#650c0c">
         <font>
            <Font name="Berlin Sans FB Bold" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor></Button>
      <Button fx:id="AddCopyBook" blendMode="DARKEN" layoutX="230.0" layoutY="200.0" mnemonicParsing="false" onAction="#addCopyOfBookEntry" prefHeight="54.0" prefWidth="127.0" text="Add copy of book" textFill="#650b0b">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="13.0" />
         </font></Button>
      <Button fx:id="signout" layoutX="236.0" layoutY="319.0" mnemonicParsing="false" onAction="#signOut" prefHeight="42.0" prefWidth="127.0" text="Signout" textFill="#611515">
         <font>
            <Font name="Berlin Sans FB Bold" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Button>
      <Button fx:id="addNewBook" layoutX="21.0" layoutY="321.0" mnemonicParsing="false" onAction="#addNewBook" prefHeight="42.0" prefWidth="161.0" text="Add Book" textFill="#681818">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
