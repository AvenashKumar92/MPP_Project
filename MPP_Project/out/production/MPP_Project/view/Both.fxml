<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500" prefWidth="400" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.SystemController">
   <children>
      <Button fx:id="AddMemberButton" layoutX="33.0" layoutY="135.0" mnemonicParsing="false" onAction="#addLibraryMemBoth" prefHeight="42.0" prefWidth="146.0" text="Add library member" textFill="#5b1414">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="13.0" />
         </font></Button>
      <Label layoutX="33.0" layoutY="44.0" prefHeight="31.0" prefWidth="328.0" text="Both's level authorization" textFill="#5e1717">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="27.0" />
         </font>
      </Label>
      <Button fx:id="addCopyBookButton" layoutX="39.0" layoutY="225.0" mnemonicParsing="false" onAction="#addCopyBookBoth" prefHeight="48.0" prefWidth="146.0" text="Add copy of book" textFill="#611414">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="15.0" />
         </font></Button>
      <Button fx:id="CheckoutBookButton" layoutX="243.0" layoutY="135.0" mnemonicParsing="false" onAction="#checkOutBookBoth" prefHeight="42.0" prefWidth="133.0" text="Checkout book" textFill="#6f1c1c">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="15.0" />
         </font></Button>
      <Button fx:id="signOut" layoutX="244.0" layoutY="225.0" mnemonicParsing="false" nodeOrientation="RIGHT_TO_LEFT" onAction="#signOut" prefHeight="48.0" prefWidth="133.0" text="Signedout" textFill="#7c1818">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="22.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
