<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500" prefWidth="400" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.SystemController">
   <children>
      <Label graphicTextGap="5.0" layoutX="56.0" layoutY="174.0" prefHeight="17.0" prefWidth="94.0" text="Member ID:" textFill="#5e1313">
         <effect>
            <Glow level="0.37" />
         </effect></Label>
      <Label layoutX="53.0" layoutY="226.0" prefHeight="17.0" prefWidth="94.0" text="ISBN of book:" textFill="#5e1313">
         <effect>
            <Glow level="0.47" />
         </effect></Label>
      <Button fx:id="CheckoutButton" graphicTextGap="5.0" layoutX="78.0" layoutY="330.0" mnemonicParsing="false" onAction="#checkoutBook" prefHeight="39.0" prefWidth="80.0" text="Submit" textFill="#572626">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
         <font>
            <Font name="Arial Rounded MT Bold" size="15.0" />
         </font></Button>
      <TextField fx:id="memberID" layoutX="165.0" layoutY="169.0" />
      <TextField fx:id="ISBN" layoutX="165.0" layoutY="221.0" onAction="#checkoutBook" />
      <Label alignment="TOP_CENTER" depthTest="ENABLE" graphicTextGap="6.0" layoutX="40.0" layoutY="35.0" prefHeight="67.0" prefWidth="272.0" text="Both Checkout Book" textFill="#853a3c" textOverrun="WORD_ELLIPSIS" wrapText="true">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="28.0" />
         </font>
      </Label>
      <Label fx:id="bookLabel" layoutX="53.0" layoutY="102.0" prefHeight="47.0" prefWidth="283.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="signout" layoutX="216.0" layoutY="330.0" mnemonicParsing="false" onAction="#backhomepageBoth" prefHeight="39.0" prefWidth="149.0" text="Back home page" textFill="#7c2939">
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
         <font>
            <Font name="Arial Rounded MT Bold" size="15.0" />
         </font></Button>
   </children>
</AnchorPane>
